    "@babel/preset-env": "^7.4.2",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "budo": "^11.6.1",
    "eslint": "^5.15.3",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "gh-pages": "^2.0.1",
    "prettier": "^1.16.4",
    "uglify-js": "^3.5.2"
  },
  "browserslist": [
